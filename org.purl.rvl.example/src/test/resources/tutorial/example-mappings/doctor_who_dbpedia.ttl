:     rdf:type owl:Ontology .
#:      owl:imports rvl: . #### uncomment this line for easier (guided) editing in editors like TopBraid

 
:SomeExampleMapping
      rvl:disabled "false"^^xsd:boolean ;
      a rvl:PropertyMapping ;
      rvl:sourceProperty dbpedia-owl:subsequentWork ;
      rvl:targetObjToObjRelation vg:Linking_Directed_Relation ; 
      .

:ExampleColorMapping
      a rvl:PropertyMapping ;
      rvl:disabled "false"^^xsd:boolean ;
 	  rvl:sourceProperty dbpprop:doctor ;
 	  rvl:targetAttribute vg:color_named ;
 	  rvl:valueMapping [
        a rvl:ValueMapping ;
		rvl:sourceValue dbpedia:Tom_Baker;
		rvl:sourceValue dbpedia:Jon_Pertwee ;
		#rvl:sourceValue "Jon Pertwee";
		#rvl:sourceValue "Tom Baker"@en;
        rvl:targetValue vg:DarkRed;
        rvl:targetValue vg:Yellow;
        #rvl:targetValue vg:Purple;
        #rvl:targetValue vg:Green;
      ]
      . 
