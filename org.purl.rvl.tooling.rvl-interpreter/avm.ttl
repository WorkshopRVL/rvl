rvl:PropertyToGraphicAttributeMapping
        a                     rdfs:Class , owl:Class , rdfs:Resource ;
        rdfs:label            "Property-to-Graphic-Attribute Mapping"^^xsd:string ;
        rdfs:subClassOf       _:b4 , rvl:PropertyMapping , _:b5 , rvl:PropertyToGraphicAttributeMapping , _:b6 , rvl:Mapping , _:b7 , _:b8 , _:b9 , _:b10 , rdfs:Resource ;
        dct:description       "A Property-to-Graphic-Attribute Mapping (P2GAM) draws a general connection between a (domain) property and the graphic attribute. The interesting part remains the mapping of the values, which is left to <a href=\"#http://purl.org/rvl/ValueMapping\">Value mappings</a> (VM). The default implicit VM, which is assumed, when no other VMs are explicitly defined, maps all source values to all target values. For details, refer to the description of VM."^^xsd:string ;
        swstatus:term_status  "stable"^^xsd:string .

viso-graphic:Separation_by_Separator_Relation
        a                     rdfs:Class , owl:Class , rdfs:Resource ;
        rdfs:comment          "A difficulty of having a separator is that the neutral value is hard to define. One group of objects is on one side of the separator, the other group on the other side, but where to put neutral elements that do not have one of the values distinguished by the separator? One solution could be to use yet another visual means (e.g. relative distance) to once again separate the first two groups from the neutral values. Another solution that may work in some cases is the following: As long as there is only one separator used (for 2D graphics) values can be positioned on the separator to have a third value, or on the (thought) continuation of a finite separator)."^^xsd:string ;
        rdfs:label            "Separation by a Separator"^^xsd:string ;
        rdfs:subClassOf       _:b11 , _:b12 , _:b13 , _:b14 , _:b15 , _:b16 , _:b17 , viso-graphic:Object-to-Object_Relation , viso-graphic:Separation_by_Separator_Relation , owl:Thing , viso-graphic:Graphic_Thing , rdfs:Resource ;
        dct:description       <http://www-st.inf.tu-dresden.de/semvis/ontologies/viso/img/vr_containment.png> ;
        swstatus:term_status  "stable"^^xsd:string .

