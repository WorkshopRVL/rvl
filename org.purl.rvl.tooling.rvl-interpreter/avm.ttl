        rdfs:label            "Metric Axis" ;
        rdfs:subClassOf       _:b44 , viso-graphic:Spatial_Reference_Object , viso-graphic:Metric_Axis , owl:Thing , _:b0 , viso-graphic:Graphic_Thing , viso-graphic:Reference_Object , viso-graphic:Graphic_Object , viso-graphic:Role , viso-graphic:Informational_Role , rdfs:Resource ;
        dct:description       "A metric axis creates a graphic space in which ratios of spatial distances, measured along the spatial dimension of the axis, are perceived as meaningful. Example: a time line."^^xsd:string ;
        swstatus:term_status  "stable"^^xsd:string .

