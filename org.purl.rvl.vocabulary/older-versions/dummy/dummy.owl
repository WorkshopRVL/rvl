<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:afn="http://jena.hpl.hp.com/ARQ/function#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns="http://purl.org/rvl/dummy/"
    xmlns:fn="http://www.w3.org/2005/xpath-functions#"
    xmlns:sp="http://spinrdf.org/sp#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:spin-ext="http://purl.org/rvl/dummy/spin-ext/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:spl="http://spinrdf.org/spl#"
  xml:base="http://purl.org/rvl/dummy/">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://spinrdf.org/spin"/>
    <spin:imports rdf:resource="http://topbraid.org/spin/rdfsplus"/>
    <owl:imports rdf:resource="http://purl.org/rvl/dummy/spin-ext/"/>
  </owl:Ontology>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/NamedShapeValue">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/rvl/dummy/NamedGraphicAttributeValue"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Named_Shape</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/NamedSizeValue">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Named Size Value</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/rvl/dummy/NamedGraphicAttributeValue"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/Author">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/rvl/dummy/DomainResource"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Author</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/Mapping">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Mapping</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/GraphicO2ORelation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >GraphicObj2ObjRelation</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/rvl/dummy/GraphicRelation"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/NamedGraphicAttributeValue">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Visual attribute</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/DomainResource">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Domain resource</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/ValueMapping">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ValueMapping</rdfs:label>
    <spin:constraint>
      <sp:Construct>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This is a customised version of spl:attribute for Source Value</rdfs:comment>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:object rdf:resource="http://spinrdf.org/spin#ConstraintViolation"/>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:subject rdf:nodeID="A0"/>
          </rdf:Description>
          <rdf:Description>
            <sp:object rdf:resource="http://spinrdf.org/spin#_this"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationRoot"/>
            <sp:subject rdf:nodeID="A0"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A0"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationPath"/>
            <sp:object>
              <rdf:Property rdf:about="http://purl.org/rvl/dummy/sourceValue"/>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >label</sp:varName>
            </sp:object>
            <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
            <sp:subject rdf:nodeID="A0"/>
          </rdf:Description>
        </sp:templates>
        <sp:where rdf:parseType="Collection">
          <sp:Union>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <rdf:rest rdf:parseType="Collection">
                  <sp:Bind>
                    <sp:expression>Error : A source value needs to be selected!</sp:expression>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >label</sp:varName>
                    </sp:variable>
                  </sp:Bind>
                </rdf:rest>
                <rdf:first>
                  <sp:Filter>
                    <sp:expression>
                      <sp:lt>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                        >1</sp:arg2>
                        <sp:arg1>
                          <spl:objectCount>
                            <sp:arg1 rdf:resource="http://spinrdf.org/spin#_this"/>
                            <sp:arg2>
                              <rdf:Property rdf:about="http://purl.org/rvl/dummy/sourceValue"/>
                            </sp:arg2>
                          </spl:objectCount>
                        </sp:arg1>
                      </sp:lt>
                    </sp:expression>
                  </sp:Filter>
                </rdf:first>
              </rdf:Description>
              <rdf:Description>
                <rdf:rest rdf:parseType="Collection">
                  <sp:Bind>
                    <sp:expression>Error : Only 1 source value may be set!</sp:expression>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >label</sp:varName>
                    </sp:variable>
                  </sp:Bind>
                </rdf:rest>
                <rdf:first>
                  <sp:Filter>
                    <sp:expression>
                      <sp:gt>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                        >1</sp:arg2>
                        <sp:arg1>
                          <spl:objectCount>
                            <sp:arg1 rdf:resource="http://spinrdf.org/spin#_this"/>
                            <sp:arg2>
                              <rdf:Property rdf:about="http://purl.org/rvl/dummy/sourceValue"/>
                            </sp:arg2>
                          </spl:objectCount>
                        </sp:arg1>
                      </sp:gt>
                    </sp:expression>
                  </sp:Filter>
                </rdf:first>
              </rdf:Description>
            </sp:elements>
          </sp:Union>
        </sp:where>
      </sp:Construct>
    </spin:constraint>
    <spin:constraint>
      <sp:Construct>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >This is a customised version of spl:attribute for target graphic relaion</rdfs:comment>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:object rdf:resource="http://spinrdf.org/spin#ConstraintViolation"/>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:subject rdf:nodeID="A1"/>
          </rdf:Description>
          <rdf:Description>
            <sp:object rdf:resource="http://spinrdf.org/spin#_this"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationRoot"/>
            <sp:subject rdf:nodeID="A1"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A1"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationPath"/>
            <sp:object>
              <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetValue"/>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >label</sp:varName>
            </sp:object>
            <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
            <sp:subject rdf:nodeID="A1"/>
          </rdf:Description>
        </sp:templates>
        <sp:where rdf:parseType="Collection">
          <sp:Union>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <rdf:rest rdf:parseType="Collection">
                  <sp:Bind>
                    <sp:expression>Error : A target visual value needs to be selected!</sp:expression>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >label</sp:varName>
                    </sp:variable>
                  </sp:Bind>
                </rdf:rest>
                <rdf:first>
                  <sp:Filter>
                    <sp:expression>
                      <sp:lt>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                        >1</sp:arg2>
                        <sp:arg1>
                          <spl:objectCount>
                            <sp:arg1 rdf:resource="http://spinrdf.org/spin#_this"/>
                            <sp:arg2>
                              <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetValue"/>
                            </sp:arg2>
                          </spl:objectCount>
                        </sp:arg1>
                      </sp:lt>
                    </sp:expression>
                  </sp:Filter>
                </rdf:first>
              </rdf:Description>
              <rdf:Description>
                <rdf:rest rdf:parseType="Collection">
                  <sp:Bind>
                    <sp:expression>Error : Only 1 target visual value may be set!</sp:expression>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >label</sp:varName>
                    </sp:variable>
                  </sp:Bind>
                </rdf:rest>
                <rdf:first>
                  <sp:Filter>
                    <sp:expression>
                      <sp:gt>
                        <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                        >1</sp:arg2>
                        <sp:arg1>
                          <spl:objectCount>
                            <sp:arg1 rdf:resource="http://spinrdf.org/spin#_this"/>
                            <sp:arg2>
                              <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetGraphicRelation"/>
                            </sp:arg2>
                          </spl:objectCount>
                        </sp:arg1>
                      </sp:gt>
                    </sp:expression>
                  </sp:Filter>
                </rdf:first>
              </rdf:Description>
              <rdf:Description>
                <rdf:rest rdf:parseType="Collection">
                  <sp:Filter>
                    <sp:expression>
                      <sp:not>
                        <sp:arg1>
                          <spl:instanceOf>
                            <sp:arg2 rdf:resource="http://purl.org/rvl/dummy/NamedGraphicAttributeValue"/>
                            <sp:arg1 rdf:parseType="Resource">
                              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >value</sp:varName>
                            </sp:arg1>
                          </spl:instanceOf>
                        </sp:arg1>
                      </sp:not>
                    </sp:expression>
                  </sp:Filter>
                  <sp:Bind>
                    <sp:expression>Error : Target visual value has to be of type VisualAttribute!</sp:expression>
                    <sp:variable rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >label</sp:varName>
                    </sp:variable>
                  </sp:Bind>
                </rdf:rest>
                <rdf:first rdf:parseType="Resource">
                  <sp:object rdf:parseType="Resource">
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >value</sp:varName>
                  </sp:object>
                  <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                  <sp:predicate>
                    <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetValue"/>
                  </sp:predicate>
                </rdf:first>
              </rdf:Description>
            </sp:elements>
          </sp:Union>
        </sp:where>
      </sp:Construct>
    </spin:constraint>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Maps a discrete property value to a discrete visual variable value. &#xD;
&#xD;
ValueMapping is a subclass of Mapping and inherits the property includeInLegend. This way, it can consistantly be defined, whether this ValueMapping should appear in a legend.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/rvl/dummy/Mapping"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/Book">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Book</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/rvl/dummy/DomainResource"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/rvl/dummy/PropertyMapping">
    <rdfs:subClassOf rdf:resource="http://purl.org/rvl/dummy/Mapping"/>
    <spin:constraint>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
            <sp:predicate>
              <rdf:Property rdf:about="http://purl.org/rvl/dummy/sourceProperty"/>
            </sp:predicate>
            <sp:object>
              <rdf:Description rdf:nodeID="A2">
                <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >sp</sp:varName>
              </rdf:Description>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
            <sp:predicate>
              <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetGraphicRelation"/>
            </sp:predicate>
            <sp:object>
              <rdf:Description rdf:nodeID="A3">
                <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >tvm</sp:varName>
              </rdf:Description>
            </sp:object>
          </rdf:Description>
          <sp:SubQuery>
            <sp:query>
              <sp:Select>
                <sp:where rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                    <sp:predicate>
                      <rdf:Property rdf:about="http://purl.org/rvl/dummy/sourceProperty"/>
                    </sp:predicate>
                    <sp:object rdf:nodeID="A2"/>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                    <sp:predicate>
                      <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetGraphicRelation"/>
                    </sp:predicate>
                    <sp:object rdf:nodeID="A3"/>
                  </rdf:Description>
                  <sp:Optional>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A2"/>
                        <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#subPropertyOf"/>
                        <sp:object>
                          <owl:DatatypeProperty rdf:about="http://purl.org/rvl/dummy/has_nominal_value"/>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A3"/>
                        <sp:predicate>
                          <rdf:Property rdf:about="http://purl.org/rvl/dummy/has_effectiveness_ranking_for_nominal_data"/>
                        </sp:predicate>
                        <sp:object>
                          <rdf:Description rdf:nodeID="A4">
                            <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >rankValue</sp:varName>
                          </rdf:Description>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject>
                          <rdf:Description rdf:nodeID="A5">
                            <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >anyOtherVm</sp:varName>
                          </rdf:Description>
                        </sp:subject>
                        <sp:predicate>
                          <rdf:Property rdf:about="http://purl.org/rvl/dummy/has_effectiveness_ranking_for_nominal_data"/>
                        </sp:predicate>
                        <sp:object>
                          <rdf:Description rdf:nodeID="A6">
                            <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >otherRankValue</sp:varName>
                          </rdf:Description>
                        </sp:object>
                      </rdf:Description>
                    </sp:elements>
                  </sp:Optional>
                  <sp:Optional>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A2"/>
                        <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#subPropertyOf"/>
                        <sp:object>
                          <rdf:Property rdf:about="http://purl.org/rvl/dummy/has_ordinal_value"/>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A3"/>
                        <sp:predicate>
                          <rdf:Property rdf:about="http://purl.org/rvl/dummy/has_effectiveness_ranking_for_ordinal_data"/>
                        </sp:predicate>
                        <sp:object rdf:nodeID="A4"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A5"/>
                        <sp:predicate>
                          <rdf:Property rdf:about="http://purl.org/rvl/dummy/has_effectiveness_ranking_for_ordinal_data"/>
                        </sp:predicate>
                        <sp:object rdf:nodeID="A6"/>
                      </rdf:Description>
                    </sp:elements>
                  </sp:Optional>
                  <sp:Optional>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A2"/>
                        <sp:predicate rdf:resource="http://www.w3.org/2000/01/rdf-schema#subPropertyOf"/>
                        <sp:object>
                          <rdf:Property rdf:about="http://purl.org/rvl/dummy/has_quantitative_value"/>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A3"/>
                        <sp:predicate>
                          <owl:DatatypeProperty rdf:about="http://purl.org/rvl/dummy/has_effectiveness_ranking_for_quantitatitive_data"/>
                        </sp:predicate>
                        <sp:object rdf:nodeID="A4"/>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:nodeID="A5"/>
                        <sp:predicate>
                          <owl:DatatypeProperty rdf:about="http://purl.org/rvl/dummy/has_effectiveness_ranking_for_quantitatitive_data"/>
                        </sp:predicate>
                        <sp:object rdf:nodeID="A6"/>
                      </rdf:Description>
                    </sp:elements>
                  </sp:Optional>
                  <sp:Filter>
                    <sp:expression>
                      <sp:and>
                        <sp:arg1>
                          <sp:bound>
                            <sp:arg1 rdf:nodeID="A4"/>
                          </sp:bound>
                        </sp:arg1>
                        <sp:arg2>
                          <sp:bound>
                            <sp:arg1 rdf:nodeID="A6"/>
                          </sp:bound>
                        </sp:arg2>
                      </sp:and>
                    </sp:expression>
                  </sp:Filter>
                  <sp:Filter>
                    <sp:expression>
                      <sp:ne>
                        <sp:arg1 rdf:nodeID="A3"/>
                        <sp:arg2 rdf:nodeID="A5"/>
                      </sp:ne>
                    </sp:expression>
                  </sp:Filter>
                  <sp:Filter>
                    <sp:expression>
                      <sp:gt>
                        <sp:arg1 rdf:nodeID="A6"/>
                        <sp:arg2 rdf:nodeID="A4"/>
                      </sp:gt>
                    </sp:expression>
                  </sp:Filter>
                </sp:where>
                <sp:resultVariables rdf:parseType="Collection">
                  <rdf:Description rdf:about="http://spinrdf.org/spin#_this"/>
                  <rdf:Description>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >altCount</sp:varName>
                    <sp:expression>
                      <sp:Count>
                        <sp:expression rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >anyOtherVm</sp:varName>
                        </sp:expression>
                      </sp:Count>
                    </sp:expression>
                  </rdf:Description>
                </sp:resultVariables>
              </sp:Select>
            </sp:query>
          </sp:SubQuery>
          <sp:Bind>
            <sp:variable>
              <rdf:Description rdf:nodeID="A7">
                <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >cvLabel</sp:varName>
              </rdf:Description>
            </sp:variable>
            <sp:expression>
              <fn:concat>
                <sp:arg1>Warning : There are </sp:arg1>
                <sp:arg2>
                  <rdf:Description rdf:nodeID="A8">
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >altCount</sp:varName>
                  </rdf:Description>
                </sp:arg2>
                <sp:arg3> more effective visual means for visualizing the selected source property</sp:arg3>
              </fn:concat>
            </sp:expression>
          </sp:Bind>
          <sp:Filter>
            <sp:expression>
              <sp:gt>
                <sp:arg1 rdf:nodeID="A8"/>
                <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >0</sp:arg2>
              </sp:gt>
            </sp:expression>
          </sp:Filter>
        </sp:where>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:nodeID="A9"/>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object rdf:resource="http://spinrdf.org/spin#ConstraintViolation"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A9"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationRoot"/>
            <sp:object rdf:resource="http://spinrdf.org/spin#_this"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A9"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationPath"/>
            <sp:object>
              <rdf:Property rdf:about="http://purl.org/rvl/dummy/targetGraphicRelation"/>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A9"/>
